echo "# Projects" >> README.md
git init
git add README.md
git commit -m "first commit"
git branch -M main
git remote add origin https://github.com/okkowittenberg/Projects.git
git push -u origin main

let keycard = 'rainbow';
let vaccinated = true;
console.log('Press \'Run\' to insert your keycard!')
if (keycard === 'yellow') {
  console.log('Access granted.');
} else if (keycard === 'green') {
  console.log('Access denied');
} else if (keycard === 'black') {
  console.log('Access denied.');
} else if (keycard === 'red') {
  console.log('Access denied. You need a yellow card.')
} else if (keycard === 'rainbow' && vaccinated) {
  console.log('You\'re gay! Access denied.')
}

if (keycard !== 'yellow') {
  console.log('Would you like to play a game to obtain the yellow keycard?');
  console.log('Type Y to play or N to cancel.');
}
let Y = true;
let N = false;
console.log('Y');
if(Y){
  console.log('Choose a number between 1-10');
}
let number = 3
if (number === 3) {
  console.log('Correct! You win a yellow keycard! Would you like to open the door?');
} else if (number > 3) {
  console.log('Incorrect! You guessed too high!');
} else if (number < 3) {
  console.log('Incorrect! You guessed too low!');
} else {
  console.log('I\'m not sure what you input to get here, but you done broke the game.')
}

if (number === 3) {
  console.log('Type Y to open or N to cancel.');
}
console.log('N');
let door = 'closed';
if (door === 'open') {
  console.log('You win!!');
} else {
  console.log('You coward! Open the door!!');
}
